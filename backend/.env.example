PORT=3000
# Use uma das opções abaixo para o Firebase Admin:
# 1) FIREBASE_ADMIN_KEY_BASE64: conteúdo do service account em base64 (recomendado)
FIREBASE_ADMIN_KEY_BASE64=
# 2) GOOGLE_APPLICATION_CREDENTIALS: caminho absoluto para o JSON do service account
GOOGLE_APPLICATION_CREDENTIALS=

# Config do Firebase Client (frontend)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_APP_ID=

NODE_ENV=development
